<body>
	<%= yield %>
	
		<!--로그인-->
<input class="btn btn-default" input type="hidden" data-target="#layerpop" data-toggle="modal"></button><br/>
<div class="modal fade" id="layerpop" >
  <div class="modal-dialog">
    <div class="modal-content">
      <!-- header -->
      <div class="modal-header">
      	<img class="htlfndr-logo navbar-brand" src="/assets/tost_logo.gif" alt="Logo" />
        <!-- 닫기(x) 버튼 -->
        <button type="button" class="close" data-dismiss="modal">×</button>
      </div>
      <!-- body -->
      <div class="modal-body">
         	<div class="form">
		    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
		    	<div style ="text-align:center"><%= flash[:alert] %></div> 
		    <div class="modal-field_id1" style="background-color:#f2f2f2">
		   	<img class="icon_user_modal" src="/assets/icon_user_id.png"/>
		   	<%= f.email_field :email, autofocus: true, :placeholder => "e-mail", :class =>"modal-field_id"%>
		    </div>
		    <div class="modal-field_id1" style="background-color:#f2f2f2">
		    <img class="icon_user_modal" src="/assets/icon_password.png"/>	
		    <%= f.password_field :password, :autocomplete => "off", :placeholder => "password", :class =>"modal-field_password"%>
		    </div>
		    <div class="actions">
		      <%= f.submit "로그인", :class => 'modal-button'%>
		      <% end %>
		  	</div>
		  	<div style="text-align:center"><%= render "devise/shared/links" %></div>
		 </div>
      </div>
      <!-- Footer -->
      <div class="modal-footer" style="text-align:auto">
        <div style="text-align:center"> 아직 토스트 회원이 아니세요? &nbsp;
        <a href="/users/sign_up"><span>회원가입</span></a></div>
      </div>
    </div>
  </div>
</div>
	
